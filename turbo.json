{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint", "lint:css"],
      "outputs": ["node_modules/.cache/eslint"]
    },
    "lint:css": {
      "dependsOn": ["^lint:css"],
      "outputs": ["node_modules/.cache/stylelint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "test": {},
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "test:coverage": {},
    "test:update": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {}
  }
}
