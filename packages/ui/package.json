{
  "name": "@stitchlet/ui",
  "version": "0.0.0",
  "description": "Accessible UI components ready to be stitched together.",
  "license": "MIT",
  "author": {
    "name": "InaCarine",
    "url": "https://github.com/InaCarine"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/InaCarine/stitchlet-ui.git",
    "directory": "packages/ui"
  },
  "keywords": [
    "components",
    "ui",
    "accessible",
    "inacarine",
    "stitchlet"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": true,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "default": "./dist/components/*/index.js"
    }
  },
  "scripts": {
    "build": "tsdown --no-cache",
    "lint": "eslint . --max-warnings 0 --no-warn-ignored --cache --cache-location node_modules/.cache/eslint",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache --cache-location node_modules/.cache/stylelint --max-warnings=0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "clean": "rm -rf node_modules .turbo"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/stylelint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@rollup/plugin-babel": "^6.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "catalog:",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "stylelint": "catalog:",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsdown": "^0.20.3",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
